{
  "name": "studenthub",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ant-design-pro": "^2.2.1",
    "antd": "^3.14.1",
    "aws-amplify": "^1.1.22",
    "aws-sdk": "^2.449.0",
    "draft-js": "^0.10.5",
    "gg-editor": "^2.0.2",
    "moment": "^2.29.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-markdown": "^4.0.6",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "react-tooltip": "^3.9.2",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --verbose",
    "eject": "react-scripts eject",
    "deploy": "aws s3 sync build/ s3://studenthub.io"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "*",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "babel-jest": "23.6.0",
    "enzyme": "*",
    "enzyme-adapter-react-16": "*",
    "identity-obj-proxy": "^3.0.0",
    "jest": "*"
  },
  "jest": {
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  }
}
